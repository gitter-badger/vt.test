<section data-ng-controller="HomeController as home">
    <!-- Global container -->
    <div class="global-container" data-ng-hide="home.showSearchOverlay">
        <!-- Left menu -->{% verbatim %}{{ home.hello }}{% endverbatim %}

        <div class="menu left">
            <div class="menu_wrapper">
                <ul>
                    <li>
                        <a href="../pages/bookingLoginPage.html">
                            <span class="icon icon-user"> </span>
                            <span class="title">{% trans %}Login{% endtrans %}</span>
                        </a>
                    </li>
                </ul>

                <ul>
                    <li class="active">
                        <a href="../pages/searchPage.html">
                            <span class="icon icon-plane-depart-big"> </span>
                            <span class="title">{% trans %}Search a flight{% endtrans %}</span>
                        </a>
                    </li>
                    <li>
                        <a href="../pages/checkinOnlinePage.html">
                            <span class="icon icon-checkin_online"> </span>
                            <span class="title">{% trans %}Online check-in{% endtrans %}</span>
                        </a>
                    </li>
                    <li>
                        <a href="http://m.booking.com/?aid=349310&label=Vliegt-nl-navtab" target="blank">
                            <span class="icon icon-hotel"> </span>
                            <span class="title">{% trans %}Book a hotel{% endtrans %}</span>
                        </a>
                    </li>
                    <li>
                        <a href="http://autohuur.vliegtickets.nl/?preflang=nl&adplat=menu&adcamp=homepage" target="blank">
                            <span class="icon icon-car"> </span>
                            <span class="title">{% trans %}Rent a car{% endtrans %}</span>
                        </a>
                    </li>
                    <li class="phone">
                        <a href="tel:09009556">
                            <span class="icon icon-phone"> </span>
                            <span class="title"><strong>0900 - 9556</strong> <span class="price-per-minute">(â‚¬ 0,45 p/m)</span></span>
                            <br />
                            <span class="subtitle"> <strong>9:00 - 17:00</strong></span>
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <span class="icon icon-logout"> </span>
                            <span class="title">{% trans %}Logout{% endtrans %}</span>
                        </a>
                    </li>
                </ul>

                <ul class="footer">
                    <li>
                        <a href="http://www.vliegtickets.nl">
                            <span class="icon icon-desktop"> </span>
                            <span class="title">{% trans %}View desktop version{% endtrans %}</span>
                        </a>
                    </li>
                </ul>

                <div style="clear: both"></div>
                <div class="menu_divider"></div>
            </div>
        </div> <!-- /Left menu -->
        <!-- <div class="menu right"> OPTIONAL RIGHT MENU </div>-->

        <!-- CONTENT_CONTAINER PUSHER -->
        <div class="content-container-pusher">

            <!-- NAVIGATION BAR -->
            <div class="navigation-bar">
                <div class="navigation-bar-overlay" id="navigation_bar_overlay"></div>
                <table>
                    <tr>
                    <div class="navigation-bar-wrapper">

                        <!-- LEFT ELEMENT -->
                        <div class="left">
                            <div class="nav_btn menu_left"></div>
                        </div>

                        <!-- MIDDLE ELEMENT -->
                        <div class="middle nopadding">
                            <div class="logo_wrapper">
                                <h1 class="logo logo_vliegtickets">
                                    Vliegtickets.nl
                                    <span></span>
                                </h1>
                            </div>
                        </div>

                        <!-- RIGHT ELEMENT -->
                        <div class="right">
                            <div class="nav_btn menu_right"></div>
                        </div>

                    </div>
                    </tr>
                </table>

            </div><!-- /NAVIGATION BAR -->

            <!-- PAGE -->
            <div class="content-container search-page">
                <div class="vt-margin">
                    <div class="form-group">
                        <div class="input-group">
                            <div class="input-group-addon">
                                <span class="glyphicon glyphicon-map-marker" aria-hidden="true"></span>
                                <span class="input-group-text">{% trans %}From{% endtrans %}:</span></div>
                            <input type="text" data-ng-model="home.search.from" class="form-control" placeholder="{% trans %}Choose your departure{% endtrans %}" data-ng-focus="home.showSearchOverlay='From'" value="Amsterdam (AMS), Nederland">
                            <div class="input-group-addon clear-btn" onclick="$j(this).prev().val('');$j(this).prev().change();;"><span class="icon icon-size-medium icon-color-suva_gray icon-close_1"></span></div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="input-group">
                            <div class="input-group-addon">
                                <span class="glyphicon glyphicon-map-marker" aria-hidden="true"></span>
                                <span class="input-group-text">{% trans %}To{% endtrans %}:</span></div>
                                <input type="text" data-ng-model="home.search.to" class="form-control" placeholder="{% trans %}Choose your destination{% endtrans %}" data-ng-focus="home.showSearchOverlay='To'">
                            <div class="input-group-addon clear-btn" onclick="$j(this).prev().val('');$j(this).prev().change();;"><span class="icon icon-size-medium icon-color-suva_gray icon-close_1"></span></span></div>
                        </div>
                    </div>
                    <div class="checkbox vt-margin-groupbottom">
                        <input id="checkbox1" type="checkbox">
                        <label for="checkbox1">
                            {% trans %}Including alternative airports{% endtrans %}
                        </label>
                    </div>
                    <div class="btn-group btn-group-justified vt-margin-groupbottom" data-toggle="buttons">
                        <label class="btn btn-default active">
                            <input type="radio" name="flightType" id="typeRoundTrip" autocomplete="off" checked>{% trans %}Return flight{% endtrans %}
                        </label>
                        <label class="btn btn-default">
                            <input type="radio" name="flightType" id="typeSingleFlight" autocomplete="off">{% trans %}Single flight{% endtrans %}
                        </label>
                    </div>
                    <div class="form-group">
                        <span class="form-label" for="date">{% trans %}Round trip{% endtrans %}</span>
                        <div class="btn-group btn-group-justified" role="group" aria-label="...">
                            <div class="btn-group" role="group">
                                <a id="dp-button" class="btn btn-default btn-icon-left" onclick="$j('#datepicker_overlay').show();$j('#global_container').hide();datepicker.showDatePicker();"><span class="icon icon-color-cerulean icon-date">&nbsp;</span><span id="startDate"></span><span class="end-date" id="endDate"></span></a>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-4">
                            <span class="form-label">{% trans %}Adults{% endtrans %}</span>
                        </div>
                        <div class="col-xs-4">
                            <span class="form-label">{% trans %}Children{% endtrans %} <span class="subtext">(2-11{% trans %}yr{% endtrans %})</span></span>
                        </div>
                        <div class="col-xs-4">
                            <span class="form-label">{% trans %}Babies{% endtrans %} <span class="subtext">(0-1{% trans %}yr{% endtrans %})</span></span>
                        </div>
                    </div>
                    <div class="row vt-margin-groupbottom">
                        <div class="col-xs-4">
                            <div class="input-group">
                                <div class="input-group-addon nopadding"><span class="icon icon-size-medium icon-color-cerulean icon-adult"></span></div>
                                <select class="form-control" id="travelerAdult" >
                                    <option value="0">0</option>
                                    <option value="1" selected>1</option>
                                    <option value="2">2</option>
                                    <option value="2">3</option>
                                    <option value="2">4</option>
                                    <option value="2">5</option>
                                    <option value="2">6</option>
                                    <option value="2">7</option>
                                    <option value="2">8</option>
                                    <option value="2">9</option>
                                    <option value="2">10</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-xs-4">
                            <div class="input-group">
                                <div class="input-group-addon nopadding"><span class="icon icon-size-medium icon-color-cerulean icon-child"></span></div>
                                <select class="form-control" id="travelerChild" >
                                    <option value="0" selected>0</option>
                                    <option value="1">1</option>
                                    <option value="2">2</option>
                                    <option value="2">3</option>
                                    <option value="2">4</option>
                                    <option value="2">5</option>
                                    <option value="2">6</option>
                                    <option value="2">7</option>
                                    <option value="2">8</option>
                                    <option value="2">9</option>
                                    <option value="2">10</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-xs-4">
                            <div class="input-group">
                                <div class="input-group-addon nopadding"><span class="icon icon-size-medium icon-color-cerulean icon-baby"></span></div>
                                <select class="form-control" id="travelerBaby" >
                                    <option value="0" selected>0</option>
                                    <option value="1">1</option>
                                    <option value="2">2</option>
                                    <option value="2">3</option>
                                    <option value="2">4</option>
                                    <option value="2">5</option>
                                    <option value="2">6</option>
                                    <option value="2">7</option>
                                    <option value="2">8</option>
                                    <option value="2">9</option>
                                    <option value="2">10</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div>
                        <div class="form-group">
                            <a class="collapsed" data-toggle="collapse" href="#extendedSearch" aria-expanded="false" aria-controls="extendedSearch">
                                <span class="collapse-show">{% trans %}Show{% endtrans %}</span><span class="collapse-hide">{% trans %}Hide{% endtrans %}</span> {% trans %}advanced search{% endtrans %}
                                <span class="icon icon-collapse pull-right">
                                    <span class="path1 icon-size-medium icon-color-hawkes_blue"></span><span class="path2 icon-size-medium icon-color-cerulean"></span>
                                </span>
                            </a>
                        </div>
                        <div class="collapse" id="extendedSearch">
                            <div class="form-group btn-group btn-group-justified" data-toggle="buttons">
                                <label class="btn btn-default active">
                                    <input type="radio" name="options" id="typeRoundTrip" autocomplete="off" checked>Economy
                                </label>
                                <label class="btn btn-default">
                                    <input type="radio" name="options" id="typeSingleFlight" autocomplete="off">Business & first
                                </label>
                            </div>
                            <div class="form-group">
                                <span class="form-label" for="airline">Luchtvaartmaatschappij</span>
                                <select class="form-control" id="airline">
                                    <option selected>Selecteer een luchtvaartmaatschappij</option>
                                    <option>KLM</option>
                                    <option>Lufthansa</option>
                                    <option>SAS</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <a onclick="$j('.vt-loading-overlay-dark').show();loader.transitionToNextPage();event.preventDefault();" href="../pages/searchResultsPage.html" class="btn btn-block btn-rounded btn-limeade btn-lg vt-margin-grouptop">Vind de beste vliegtickets</a>
                </div>
                <ul class="fullwidth-list">
                    <li>
                        <a href="../pages/checkinOnlinePage.html">
                            <span class="icon icon-size-medium icon-color-cerulean icon-checkin_online"></span>
                            Online inchecken
                            <span class="icon icon-size-medium icon-color-suva_gray icon-list_arrow"></span>
                        </a>
                    </li>
                    <li>
                        <a href="http://m.booking.com/?aid=349310&label=Vliegt-nl-navtab" target="blank"><span class="icon icon-size-medium icon-color-cerulean icon-hotel"></span>
                            Boek hotel<span class="icon icon-size-medium icon-color-suva_gray icon-list_arrow"></span>
                        </a>
                    </li>
                    <li>
                        <a href="http://autohuur.vliegtickets.nl/?preflang=nl&adplat=menu&adcamp=homepage" target="blank"><span class="icon icon-size-medium icon-color-cerulean icon-car"></span>
                            Boek huurauto<span class="icon icon-size-medium icon-color-suva_gray icon-list_arrow"></span>
                        </a>
                    </li>
                    <li class="phone">
                        <a href="tel:09009556"><span class="icon icon-size-medium icon-color-cerulean icon-phone"></span>
                            0900 - 9556 <span class="subtitle">(â‚¬ 0,45 p/m)</span>
                        </a>
                    </li>
                </ul>
                <div class="view-desktop">
                    <a href="http://www.vliegtickets.nl">
                        <span class="icon icon-size-medium icon-color-cerulean icon-desktop_invert"></span>
                        Bekijk desktopversie
                    </a>
                </div>
            </div><!-- /PAGE -->
        </div><!-- /CONTAINER PUSHER -->
    </div><!-- /GLOBAL CONTAINER -->

    <search-overlay search="home.search" show-search-overlay="home.showSearchOverlay"></search-overlay>


    <div id="loading_overlay" class="vt-loading-overlay">
        <div class="spinner">
            <img class="spinner-icon" src="../assets/img/loader.svg" alt="">
        </div>
    </div>
    <div id="loading_overlay" class="vt-loading-overlay">
        <div class="spinner">
            <img class="spinner-icon" src="../assets/img/loader.svg" alt="">
        </div>
    </div>
    <div class="vt-loading-overlay vt-loading-overlay-dark"></div>
    <div class="next-page-overlay searching" id="loader-nextpage">
        <div class="loader">
            <div class="progress vt-margin">
                <div class="progress-bar vt-progress-bar-international-orange" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%">
                </div>
            </div>
            <div class="ad-image text-center">
                <img id="ad-image" class="airmiles-image" src="../assets/img/loader-airmiles.png">
            </div>
            <div class="vt-margin-left vt-margin-right vt-margin-top text-center">
                Wij zoeken de <span class="bold">beste tarieven</span> voor u,<br>een ogenblik geduld!
            </div>
            <ul class="vt-list vt-list-border-bottom">
                <li>
                    <div class="row">
                        <span class="col-xs-2 icon icon-color-cerulean icon-plane-depart-big"></span>
                        <span class="col-xs-8">
                            Amsterdam <span class="bold">(AMS)</span><br>
                            Paramaribo <span class="bold">(PBM)</span><br>
                            <span class="bold">7.11.2014</span><br>
                        </span>
                    </div>
                </li>
                <li>
                    <div class="row">
                        <span class="col-xs-2 icon icon-color-cerulean icon-plane_return_big"></span>
                        <span class="col-xs-8">
                            Paramaribo <span class="bold">(PBM)</span><br>
                            Amsterdam <span class="bold">(AMS)</span><br>
                            <span class="bold">15.11.2014</span><br>
                        </span>
                    </div>
                </li>
            </ul>
            <div class="text-center safe-booking vt-margin-bot">
                Veilig online boeken met:<br>
                <div class="bg-loading-footer"></div>
            </div>
        </div>
    </div>
    <div id="datepicker_overlay" class="overlay">
        <div class="navigation-bar text-center">
            <div class="row">
                <div class="col-xs-2">
                    <span class="icon icon-size-medium icon-color-white icon-close_1 close-btn" onclick="$j('#datepicker_overlay').hide();$j('#global_container').show();datepicker.hideDatePicker();datepicker.reset();"></span>
                </div>
                <div class="col-xs-8 text-center">
                    Kies je vliegdata
                </div>
            </div>
        </div>
        <div id="datePicker"></div>
    </div><!-- /DatePicker -->
</section>
